{
    "name": "jaybizzle/safeurl",
    "description": "A Laravel package to create safe, SEO friendly urls",
    "authors": [
        {
            "name": "Mark Beech",
            "email": "mbeech@mark-beech.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/config": "4.*"
    },
    "require-dev": {
        "orchestra/testbench": "2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Jaybizzle\\Safeurl\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
